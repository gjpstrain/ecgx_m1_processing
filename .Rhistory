library(tidyverse)
anonymise_function <- function (df, passed_df, identifier) {
data <- read_csv(df)
passed <- read_csv(passed_df)
passed_yes <- passed %>%
filter(passed == TRUE)
just_passed <- inner_join(data, passed_yes, by = "participant")
final_data <- just_passed %>%
group_by(participant) %>%
dplyr::mutate(ID = cur_group_id())
final_data$participant <- NULL
final_data <- final_data %>%
rename(participant = ID)
write_csv(final_data, paste0("experimental_data/ECG_X_pilot_r2_", identifier,".csv"))
#write_csv(final_data, "experimental_data/ECG_X_pilot_r2_J.csv")
}
